{
  "name": "monica",
  "version": "0.2.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "android": "node node_modules/react-native/local-cli/cli.js run-android",
    "ios": "node node_modules/react-native/local-cli/cli.js run-ios",
    "test": "jest",
    "lint": "tslint -p tsconfig.json",
    "ios:beta": "npm run test && npm run version:ios && cd ios && bundle exec fastlane beta",
    "android:alpha": "npm run test && npm run version:android && cd android && bundle exec fastlane alpha",
    "android:beta": "npm run test && npm run version:android && cd android && bundle exec fastlane beta",
    "version:ios": "cd ios && bundle exec fastlane version version:$npm_package_version",
    "version:android": "cd android && bundle exec fastlane version version:$npm_package_version",
    "clean": "watchman watch-del-all && rm -rf node_modules && yarn && rm -rf $TMPDIR/react-* && rm -rf $TMPDIR/haste-map-react-native-packager-*"
  },
  "dependencies": {
    "appcenter": "~1.3.0",
    "appcenter-analytics": "~1.3.0",
    "appcenter-crashes": "~1.3.0",
    "babel-core": "~6.26.0",
    "base-64": "~0.1.0",
    "formik": "~0.11.1",
    "frisbee": "~1.6.4",
    "moment": "~2.20.1",
    "react": "~16.3.0-alpha.1",
    "react-native": "0.54.2",
    "react-native-actionsheet": "~2.3.0",
    "react-native-code-push": "~5.2.2",
    "react-native-device-info": "~0.21.0",
    "react-native-keyboard-aware-scroll-view": "~0.5.0",
    "react-native-navbar": "~2.1.0",
    "react-native-safe-area-view": "~0.7.0",
    "react-native-search-bar": "~3.4.0",
    "react-native-searchbar": "~1.14.0",
    "react-native-splash-screen": "~3.0.6",
    "react-native-svg": "~6.2.2",
    "react-native-svg-charts": "~3.0.2",
    "react-native-vector-icons": "~4.5.0",
    "react-navigation": "~1.2.1",
    "react-navigation-redux-helpers": "~1.0.1",
    "react-redux": "~5.0.7",
    "redux": "~3.7.2",
    "redux-debounced": "~0.4.0",
    "redux-thunk": "~2.2.0",
    "tslib": "~1.9.0",
    "typesafe-actions": "~1.1.2",
    "utility-types": "~1.1.0"
  },
  "devDependencies": {
    "@types/cheerio": "^0.22.7",
    "@types/jest": "^22.2.0",
    "@types/react": "^16.0.40",
    "@types/react-native": "^0.52.18",
    "@types/react-navigation": "^1.2.3",
    "@types/react-redux": "^5.0.15",
    "babel-eslint": "^8.2.2",
    "enzyme": "~3.3.0",
    "enzyme-adapter-react-16": "~1.1.1",
    "enzyme-to-json": "~3.3.1",
    "jest": "~22.4.2",
    "prettier": "~1.11.0",
    "react-dom": "~16.2.0",
    "react-native-typescript-transformer": "~1.2.3",
    "react-test-renderer": "16.3.0-alpha.1",
    "ts-jest": "~22.4.2",
    "tslint": "~5.9.1",
    "tslint-config-prettier": "~1.10.0",
    "tslint-plugin-prettier": "~1.3.0",
    "tslint-react": "~3.5.1",
    "typescript": "~2.7.2"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "<rootDir>setupTests.js",
    "preset": "react-native",
    "transform": {
      "^.+\\.ts?$": "ts-jest",
      "^.+\\.tsx?$": "ts-jest"
    },
    "testMatch": [
      "**/*.spec.{ts,tsx}"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "coverageReporters": [
      "text",
      "lcov"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}"
    ],
    "moduleNameMapper": {
      "@src/(.*)": "<rootDir>/src/$1",
      "^image![a-zA-Z0-9$_-]+$": "GlobalImageStub",
      "^[@./a-zA-Z0-9$_-]+\\.(png|gif)$": "RelativeImageStub"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!(jest-)?react-native|react-navigation)"
    ]
  }
}
